"""


Entrada = input()

auxiliar = Entrada.split(" ")


Competidores = int(auxiliar[0])
QuantidadeFolhasEspecial = int(auxiliar[1])
QuantidadeFolhasCompetidor = int(auxiliar[2])

if((QuantidadeFolhasEspecial / Competidores) >= QuantidadeFolhasCompetidor):
    print("S")
else:
    print("N")
